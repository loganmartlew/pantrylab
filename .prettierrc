{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "lf",
  "importOrder": ["^@pantrylab/(.*)$", "<THIRD_PARTY_MODULES>", "<BUILTIN_MODULES>", "^[.]", "<TYPES>"],
  "importOrderParserPlugins": [
    "classProperties",
    "decorators-legacy",
    "typescript",
    "jsx"
  ],
  "importOrderTypeScriptVersion": "5.0.0",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
